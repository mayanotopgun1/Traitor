{
  "fuzzer": {
    "iterations": 50000,  
    "mutations_per_seed": 10,
    "seed_selection_strategy": "random",
    "max_time_per_case_sec": 10,
    "strategy_weights": {
      "ast_structural": 1.0,
      "ast_non_structural": 0.0,
      "llm_injection": 0.0
    },
    "structural_subweights": {
      "constraint_injection": 0.70,
      "add_impl": 0.15,
      "add_trait": 0.10,
      "add_assoc_type": 0.05
    },
    "stall_threshold": 10
  },
  "run": {
    "structural_only": true,
    "max_cases": 5000,
    "max_results_gb": 30,

    "min_free_gb": 40,

    "keep_success_cases": 2000,
    "keep_error_cases": 2000,

    "promote_success": true,
    "new_seeds_max": 2000,
    "new_seeds_prefix": "new"
  },
  "paths": {
    "seeds": "seeds",
    "results": "results_night",
    "logs": "logs_night"
  },
  "compiler": {
    "enable_next_trait_solver": false,
    "rustc_cmd": ["rustc"],
    "rustc_z_cmd": ["rustc", "+nightly"],
    "next_trait_solver_flag": "-Znext-solver=coherence"
  },
  "llm": {
    "provider": "mock",
    "model": "gpt-4",
    "api_key_env": "OPENAI_API_KEY"
  }
}
