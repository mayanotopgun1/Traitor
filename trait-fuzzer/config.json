{
    "fuzzer": {
        "iterations": 50000,
        "workers": 4,
        "worker_console_logging": false,
        "mutations_per_seed": 8,
        "injection_mutations_per_round": 30,
        "projection_mutations_per_round": 30,
        "lifetime_mutations_per_round": 20,
        "outlive_mutations_per_round": 20,
        "structural_mutations_per_round": 1,
        "seed_selection_strategy": "ttdn_metric",
        "max_time_per_case_sec": 10,
        "structural_subweights": {
            "add_impl": 0.50,
            "add_trait": 0.50
        },
        "max_promotions_per_seed": 6
    },
    "run": {
        "structural_only": false,
        "max_cases": null,
        "max_results_gb": null,
        "min_free_gb": null,
        "keep_success_cases": 500,
        "keep_error_cases": 500,
        "keep_fate_cases": 500,
        "detect_miscompilation": false,
        "promote_success": true,
        "new_seeds_max": 2000,
        "new_seeds_prefix": "New"
    },
    "paths": {
        "seeds": "seeds",
        "results": "results",
        "logs": "logs"
    },
    "compiler": {
        "enable_next_trait_solver": true,
        "enable_nightly_compile": true,
        "rustc_cmd": [
            "rustc"
        ],
        "rustc_z_cmd": [
            "rustc",
            "+nightly"
        ],
        "next_trait_solver_flag": "-Znext-solver=globally",
        "parallel_compile": true,
        "parallel_workers": 3
    },
    "llm": {
        "provider": "mock",
        "model": "gpt-4",
        "api_key_env": "OPENAI_API_KEY"
    }
}