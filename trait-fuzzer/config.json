{
    "fuzzer": {
        "iterations": 50000,
        "mutations_per_seed": 20,
        "seed_selection_strategy": "ttdn_metric",
        "max_time_per_case_sec": 50,
        "strategy_weights": {
            "ast_structural": 0.5,
            "ast_non_structural": 0.5,
            "llm_injection": 0.0
        },
        "structural_subweights": {
            "constraint_injection": 0.70,
            "add_impl": 0.15,
            "add_trait": 0.10,
            "add_assoc_type": 0.05
        },
        "stall_threshold": 10,
        "max_promotions_per_seed": 2
    },
    "run": {
        "structural_only": true,
        "max_cases": null,
        "max_results_gb": null,
        "min_free_gb": null,

        "keep_success_cases": 100,
        "keep_error_cases": 100,

        "promote_success": false,
        "new_seeds_max": 1000,
        "new_seeds_prefix": "New"
    },
    "paths": {
        "seeds": "seeds",
        "results": "results",
        "logs": "logs"
    },
    "compiler": {
        "enable_next_trait_solver": true,
        "enable_nightly_compile": true,
        "rustc_cmd": ["rustc"],
        "rustc_z_cmd": ["rustc", "+nightly"],
        "next_trait_solver_flag": "-Znext-solver=globally"
    },
    "llm": {
        "provider": "mock",
        "model": "gpt-4",
        "api_key_env": "OPENAI_API_KEY"
    }
}